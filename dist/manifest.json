{
  "manifest_version": 2,
  "name": "Goto Symbol",
  "version": "0.0.1",
  "description": "Goto definition tags on pages",
  "icons":{
    "16": "icon_16.png",
    "48": "icon_32.png",
    "128": "icon_128.png"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["bundle.js"],
    "css": ["horsey.min.css", "style.css"]
  }],
  "permissions": ["*://*/*"],
  "web_accessible_resources": ["style.css", "horsey.min.css"]
}
